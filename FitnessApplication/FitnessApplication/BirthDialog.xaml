<Window x:Class="FitnessApplication.BirthDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessApplication"
        mc:Ignorable="d"
        Title="BirthDialog" Height="250" Width="300" FontFamily="Cambria" FontSize="12">
    <Grid Margin="0,0,3.333,1.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="179*"/>
            <ColumnDefinition Width="111*"/>
        </Grid.ColumnDefinitions>
        <DatePicker HorizontalAlignment="Left"
                    Margin="35,61,0,0"
                    VerticalAlignment="Top"
                    SelectedDateChanged="DatePicker_SelectedDateChanged" Height="25" Width="122"/>
        <Button FontStyle="Normal" FontStretch="UltraCondensed"  Background="GhostWhite" Content="OK" HorizontalAlignment="Left" Margin="25.667,188,0,10" Width="75" Click="Button_Click" Grid.Column="1"/>
    </Grid>
</Window>
